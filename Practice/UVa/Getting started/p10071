#include<bits/stdc++.h>

using namespace std;

int main(){
	ios::sync_with_stdio(0);
	cin.tie(0);
	int n,v;
	while((scanf("%d%d",&n,&v))!=EOF){
		cout<<n*(2*v)<<endl;
	}
	
	
	return 0;
}
